  "devDependencies": {
    "cross-env": "5.2.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-jest": "22.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "jest-dom": "3.0.1",
    "lint-staged": "8.1.1",
    "prettier": "1.16.3",
    "react-testing-library": "6.1.2"
  }
